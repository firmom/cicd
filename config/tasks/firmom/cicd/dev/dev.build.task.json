{
  "name": "BuildDevFirmomCICD",
  "entrypoint": true,
  "extends": ["CloneDevFirmomCICD"],
  "locks": ["/data/code/firmom/cicd-dev"],
  "executor": "onlylast",
  "trigger": [],
  "script": {
    "cwd": "/data/code/firmom/cicd-dev",
    "commands": [{
      "command": "docker",
      "args": ["build", ".", "-t", "firmom/cicd:dev"]
    }, {
      "command": "sh",
      "args": ["-x", "/app/config/helpers/docker-push.sh", "firmom/cicd:dev"]
    }]
  }
}