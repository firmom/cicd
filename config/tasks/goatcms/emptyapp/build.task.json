{
  "name": "BuildGoatcmsEmptyapp",
  "entrypoint": true,
  "extends": ["CloneGoatcmsEmptyapp"],
  "locks": ["code/goatcms/emptyapp"],
  "executor": "onlylast",
  "trigger": [],
  "script": {
    "cwd": "/go/src/github.com/goatcms/emptyapp",
    "commands": [{
      "cwd": ".",
      "command": "sh",
      "args": ["-x", "config/tasks/goatcms/emptyapp/build-secrets.sh"]
    }, {
      "command": "goatcli",
      "args": ["build", "--interactive=false"]
    }, {
      "command": "dep",
      "args": ["ensure"]
    }, {
      "command": "go",
      "args": ["build", "-o", "/data/archive/goatcms/emptyapp/emptyapp-latest-dev", "./main.go"]
    }]
  }
}
