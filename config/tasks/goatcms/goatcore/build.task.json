{
  "name": "BuildGoatcmsGoatcore",
  "entrypoint": true,
  "extends": ["CloneGoatcmsGoatcore"],
  "locks": [],
  "executor": "onlylast",
  "trigger": ["BuildGoatcmsGoatcms", "BuildGoatcmsGoatcli"],
  "script": {
    "cwd": "/go/src/github.com/goatcms/goatcore",
    "commands": [{
      "command": "dep",
      "args": ["ensure"]
    }, {
      "cwd": ".",
      "command": "sh",
      "args": ["-x", "config/tasks/goatcms/goatcore/build-secrets.sh"]
    }, {
      "command": "go",
      "args": ["test", "./..."]
    }]
  }
}
