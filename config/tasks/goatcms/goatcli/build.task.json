{
  "name": "BuildGoatcmsGoatcli",
  "entrypoint": true,
  "extends": ["CloneGoatcmsGoatcli"],
  "locks": ["data/code/goatcms/goatcli"],
  "executor": "onlylast",
  "trigger": [],
  "script": {
    "cwd": "data/code/goatcms/goatcli",
    "commands": [{
      "command": "go",
      "args": ["test", "./cliapp/..."]
    }, {
      "command": "go",
      "args": ["build", "./main", "-o", "goatcli"]
    }, {
      "command": "rm",
      "args": ["/go/bin/goatcli"]
    }, {
      "command": "mv",
      "args": ["./goatcli", "/go/bin"]
    }]
  }
}
